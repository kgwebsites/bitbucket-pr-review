{
    "manifest_version": 2,
    "name": "BitBucket Pull Request Review Helper",
    "description": "This extension allows you to flag individual files in a pull request as reviewed",
    "icons": {
        "16": "bitbucket-pr-review16.png",
        "48": "bitbucket-pr-review48.png",
       "128": "bitbucket-pr-review128.png"
    },
    "version": "2.0.0",
    "homepage_url": "https://github.com/kgwebsites/bitbucket-pr-review",
    "author": "Kyle Goss",
    "key": "",
    "content_scripts": [
        {
            "matches": ["https://bitbucket.org/*/pull-requests/*"],
            "css": [
                "src/style.css"
            ],
            "js": [
                "src/dataStore.js",
                "src/fileDiff.js",
                "src/hash.js",
                "src/init.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ],
    "options_ui": {
        "page": "src/options.html",
        "open_in_tab": false
    }
}
